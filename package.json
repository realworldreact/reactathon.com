{
  "name": "immortan-boilerplate",
  "description": "A static site boilerplate",
  "version": "0.0.1",
  "main": "server/server.js",
  "scripts": {
    "lint": "eslint .",
    "start": "gulp",
    "posttest": "npm run lint",
    "clean": "rimraf public",
    "build": "npm run clean && NODE_ENV=production webpack",
    "predeploy": "npm run build && npm run cname",
    "deploy": "node gh-pages",
    "cname": "[[ -e CNAME ]] && cp CNAME public/CNAME || echo 'no CNAME found'"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    },
    "immortan": {
      "title": "Berks' Immortan Boilerplate | You will ride eternal, shiny and chrome.",
      "prefix": "immortan-boilerplate"
    }
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-runtime": "^6.11.6",
    "browser-sync": "^2.15.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.22.0",
    "debug": "^2.2.0",
    "eslint": "^3.4.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "ghooks": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-render-react": "^3.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.5.0",
    "gulp-util": "^3.0.7",
    "json-loader": "^0.5.4",
    "kouto-swiss": "^0.12.0",
    "lodash": "^4.15.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-static-webpack-plugin": "https://github.com/BerkeleyTrue/react-static-webpack-plugin.git#immortan",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-observable": "^0.12.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.0-beta.12",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "yargs": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BerkeleyTrue/immortan-boilerplate"
  },
  "license": "BSD-3-Clause"
}
